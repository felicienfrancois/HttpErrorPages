    "429": {
        "title": "Trop de requètes simultanées",
        "message": "Le serveur est surchargé et ne peut pas répondre à toutes les requètes."
    },
    
    // internal server error
    "500": {
        "title": "Erreur technique",
        "message": "Une erreur inattendue est survenue lors du traitement de la requète." 
    },
    
